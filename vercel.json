{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node@3.0.0"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}